<core:FragmentDefinition 
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay354"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Order Details"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="order num." />
				<Text id="nameText" text="123131" />
				<Label text="delivery date" />
				<Text text="{Street} {HouseNumber}" />
				<Label text="company name" />
				<Text text="{ZIPCode} {City}" />
				<Label text="status" />
				<Text id="countryText" text="{Country}" />
			
				<l:VerticalLayout class="sapUiContentPadding" width="100%">
					<Label text="new order prodct" />
				<Input
				id="productName"
			value=""
			description="product name"
			width="200px"
			fieldWidth="60px"
				
			class="sapUiSmallMarginBottom" />
					<Input
			id= "amount"		
			value=""
			description="amount"
			width="200px"
			fieldWidth="60px"
			class="sapUiSmallMarginBottom" />
					<Input
			id="price"
			value="20"
			description="price"
			width="200px"
			fieldWidth="60px"
			class="sapUiSmallMarginBottom" />
					</l:VerticalLayout>
<Button type="Reject" text="{i18n>nav2}"  id="btnin" press="create_newOrder"/>



				
				
				
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>