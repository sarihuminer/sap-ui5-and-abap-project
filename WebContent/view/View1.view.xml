<mvc:View controllerName="Ztest.Ztest.controller.View1" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					<ComboBox id="cmboxCategories"
				items="{
					path: '/Categories',
					sorter: { path: 'text' }
				}">
				<core:Item key="{IdCategory}" text="{CategoryName}" />
			</ComboBox>
				<Button type="Reject" text="{i18n>nav2}"  id="btnin" press="navTOPage2"/>
				<Button type="Reject" text="{i18n>allOrders}"  id="allOrders" press="show_all_order"/>
					<Button text="{i18n>btn2}" press="onPress" ></Button>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>